{
  "private": true,
  "name": "api",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "backup": "ts-node --project ../../tsconfig.json --require tsconfig-paths/register src/Backup.ts",
    "generate:code": "TS_NODE_TRANSPILE_ONLY=true gql-gen",
    "temp": "ts-node --project ../../tsconfig.json --require tsconfig-paths/register src/Temp.ts",
    "start": "cp -R data/backup dist/api/data && NODE_ENV=dev tsc -w & nodemon --config ./nodemon.json",
    "build": "tsc",
    "deploy": "npm run build && serverless deploy"
  },
  "dependencies": {
    "@grapheng/time": "^2.0.1",
    "@grapheng/units": "0.0.5",
    "apollo-server": "^2.8.1",
    "apollo-server-express": "^2.8.1",
    "apollo-server-lambda": "^2.8.1",
    "aws-sdk": "^2.512.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cross-fetch": "^2.2.2",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "fp-ts": "^1.8.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "raw-archive": "file:../raw-archive",
    "twilio": "^3.23.2",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@grapheng/prelude": "^10.0.0",
    "@graphql-codegen/add": "^1.8.2",
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/typescript": "^1.8.2",
    "@graphql-codegen/typescript-resolvers": "^1.8.2",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.2",
    "@types/lodash": "^4.14.116",
    "@types/moment-timezone": "^0.5.8",
    "@types/stringify-object": "^3.2.0",
    "@types/uuid": "^3.4.5",
    "nodemon": "^1.19.1",
    "serverless": "^1.50.0",
    "serverless-webpack": "^5.3.1",
    "source-map-support": "^0.5.13",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2",
    "webpack": "^4.39.2"
  }
}
